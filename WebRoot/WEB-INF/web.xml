<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>INSO</display-name>
  <servlet>
    <servlet-name>EngineServlet</servlet-name>
    <servlet-class>Engine.EngineServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>Engine.AdminServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DictServlet</servlet-name>
    <servlet-class>Engine.DictServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MobileEngineServlet</servlet-name>
    <servlet-class>Engine.MobileEngineServlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>EngineServlet</servlet-name>
    <url-pattern>/EngineServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/AdminServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DictServlet</servlet-name>
    <url-pattern>/DictServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MobileEngineServlet</servlet-name>
    <url-pattern>/MobileEngineServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>